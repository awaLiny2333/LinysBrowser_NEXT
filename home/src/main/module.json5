{
  "module": {
    "name": "home",
    "type": "entry",
    "srcEntry": "./ets/homeabilitystage/HomeAbilityStage.ets",
    "description": "$string:module_desc",
    "mainElement": "HomeAbility",
    "deviceTypes": [
      "default",
      "tablet",
      "2in1",
      //      "wearable"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "HomeAbility",
        "srcEntry": "./ets/homeability/HomeAbility.ets",
        "description": "$string:HomeAbility_desc",
        "icon": "$media:layered_image",
        "launchType": "specified",
        "label": "$string:HomeAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "removeMissionAfterTerminate": true,
        "orientation": "auto_rotation_restricted",
        "continuable": true,
        "skills": [
          {
            "entities": [
              "entity.system.home",
            ],
            "actions": [
              "action.system.home",
            ],
            "uris": [
              {
                "type": "general.hyperlink",
              }
            ]
          },
          {
            "entities": [
              "entity.system.browsable"
            ],
            "actions": [
              "ohos.want.action.viewData"
            ],
            "uris": [
              //              {
              //                "scheme": "file",
              //                "type": "text/html",
              //                // Accept html
              //                "linkFeature": "FileOpen"
              //              },
              {
                "scheme": "https"
              },
              {
                "scheme": "http"
              }
            ]
          }
        ],
        "minWindowHeight": 260,
        "minWindowWidth": 360
      }
    ],
    "extensionAbilities": [
      {
        "name": "HomeBackupAbility",
        "srcEntry": "./ets/homebackupability/HomeBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ]
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET",
        "reason": "$string:permission_reason_INTERNET",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO",
        "reason": "$string:permission_reason_INTERNET",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
      {
        "name": "ohos.permission.PRINT",
        "reason": "$string:permission_reason_PRINT",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
      {
        "name": "ohos.permission.MICROPHONE",
        "reason": "$string:permission_reason_MICROPHONE",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
      {
        "name": "ohos.permission.CAMERA",
        "reason": "$string:permission_reason_CAMERA",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
      {
        "name": "ohos.permission.WINDOW_TOPMOST",
        "reason": "$string:permission_reason_CAMERA",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
      {
        "name": "ohos.permission.ACCESS_FIDO2_ONLINEAUTH",
        "reason": "$string:permission_reason_ACCESS_FIDO2_ONLINEAUTH",
        "usedScene": {
          "when": "inuse",
          "abilities": [
            "HomeAbility"
          ]
        }
      },
      //            {
      //              "name" : "ohos.permission.READ_PASTEBOARD",
      //              "reason": "$string:permission_reason_READ_PASTEPOARD",
      //              "usedScene": {
      //                "when": "inuse",
      //                "abilities": [
      //                  "HomeAbility"
      //                ]
      //              }
      //            }
    ]
  }
}